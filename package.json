{
  "name": "apollo-codegen",
  "version": "0.8.2",
  "description": "Generate client code based on a GraphQL schema and query documents",
  "main": "./lib/index.js",
  "bin": "./lib/cli.js",
  "scripts": {
    "compile": "babel src --out-dir lib --source-maps",
    "watch": "babel src --out-dir lib --watch --source-maps",
    "prepublish": "rm -rf lib && npm run compile",
    "test": "mocha --recursive --compilers js:babel-register",
    "example" : "npm run compile &&  lib/cli.js generate examples/*.graphql --schema examples/schema.json --output examples/example.java" 
  },
  "repository": {
    "type": "git",
    "url": "apollostack/apollo-codegen"
  },
  "author": "Martijn Walraven <martijn@martijnwalraven.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.16.3",
    "chai": "^3.5.0",
    "chai-subset": "^1.3.0",
    "common-tags": "^1.3.0",
    "glob": "^7.0.5",
    "mocha": "^3.1.2"
  },
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "source-map-support": "^0.4.2",
    "change-case": "^3.0.0",
    "graphql": "^0.7.0",
    "inflected": "^1.1.7",
    "mkdirp": "^0.5.1",
    "node-fetch": "^1.5.3",
    "yargs": "^6.0.0"
  }
}
